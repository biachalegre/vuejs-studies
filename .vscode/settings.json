{
  "editor.fontFamily": "cascadia code",
  "editor.fontLigatures": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.minimap.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "dart.openDevTools": "flutter",
  "terminal.integrated.detectLocale": "off",
  "dart.debugExternalLibraries": false,
  "dart.debugSdkLibraries": false,
  "editor.renderControlCharacters": true,
  "editor.fontSize": 15,
  "editor.lineHeight": 24,
  "editor.letterSpacing": 0.5,
  "editor.fontWeight": "normal",
  "prettier.eslintIntegration": true,
  "editor.cursorWidth": 2,
  "editor.renderWhitespace": "none",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootPath}",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.maxTokenizationLineLength": 500,
  "dart.lineLength": 120,
  "dart.maxLogLineLength": 120,
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "files.associations": {
    "*.txt": "cpp"
  },
  "editor.stickyScroll.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "java.templates.typeComment": [],
  "security.workspace.trust.untrustedFiles": "open",
  "json.schemas": [],
  "code-runner.runInTerminal": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#a1a1a193",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#a1a1a193",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#a1a1a193",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#a1a1a193",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#a1a1a193",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "better-comments.multilineComments": true,
  "debug.console.wordWrap": false,
  "editor.wordWrapColumn": 150,
  "window.commandCenter": true,
  "editor.indentSize": "tabSize",
  "editor.stickyTabStops": true,
  "material-icon-theme.files.associations": {},
  "editor.codeActionsOnSave": {},
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.flutterCreateIOSLanguage": "swift",
  "dart.flutterCreatePlatforms": ["android", "ios"],
  "dart.flutterCreateOrganization": "com.bia",
  "git.confirmSync": false,
  "editor.formatOnType": true,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "editor.formatOnPaste": true,
  "editor.selectionHighlight": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "[php]": {
    "editor.defaultFormatter": "DEVSENSE.phptools-vscode"
  },
  "git.suggestSmartCommit": false,
  "redhat.telemetry.enabled": false,
  "java.import.gradle.java.home": "C:\\Program Files\\Java\\jdk-17",
  "workbench.colorTheme": "Nebula Pandas Theme",
  "explorer.compactFolders": false,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "workbench.iconTheme": "material-icon-theme",
  "debug.javascript.autoAttachFilter": "always",
  "git.openRepositoryInParentFolders": "never",
  "html.format.extraLiners": "",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.ignoreRebaseWarning": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.bracketPairColorization.enabled": false,
  "workbench.settings.applyToAllProfiles": [],
  "editor.matchBrackets": "always",
  "editor.inlayHints.enabled": "off",
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "editor.guides.indentation": true,
  "editor.guides.highlightActiveBracketPair": true
}
